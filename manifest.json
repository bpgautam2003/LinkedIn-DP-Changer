{
    "name": "LinkedIn DP changer",
    "description": "Change the profile picture of all the posts on LinkedIn homepage",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/feed/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "newpic.jpg"
            ],
            "matches": [
                "https://www.linkedin.com/*"
            ]
        }
    ]
}